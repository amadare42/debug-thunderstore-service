{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "File": {
      "Path": "app.log",
      "Append": true,
      "MinLevel": "Debug",  // min level for the file logger
      "FileSizeLimitBytes": 10240,  // use to activate rolling file behaviour
      "MaxRollingFiles": 3  // use to specify max number of log files
    }
  },
  "AllowedHosts": "*",
  "DefaultOwner": "Nobody"
}
